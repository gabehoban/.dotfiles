{
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.lineHeight": 1.2,
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.wordWrap": "on",
  "prettier.singleQuote": true,
  "files.insertFinalNewline": true,
  "files.autoSave": "onFocusChange",
  "editor.renderWhitespace": "none",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "workbench.enableExperiments": false,
  "editor.tabCompletion": "on",
  "editor.trimAutoWhitespace": true,
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "java.errors.incompleteClasspath.severity": "ignore",
  "editor.fontSize": 18,
  "editor.largeFileOptimizations": false,
  "code-runner.runInTerminal": true,
  "terminal.integrated.rendererType": "dom",
  "files.associations": {
    "Tapfile*": "ruby",
    "Caskfile*": "ruby"
  },
  "window.titleBarStyle": "native",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": "material-icon-theme",
  "todo-tree.tree.showScanModeButton": false,
  "todo-tree.highlights.enabled": true,
  "cSpell.userWords": [
    "Balrog",
    "Cyberdeamon",
    "Cyberdemon",
    "Hoban",
    "cout",
    "hitpoints",
    "hobang"
  ],
  "javascript.format.insertSpaceAfterConstructor": true,
  "editor.wrappingIndent": "deepIndent",
  "java.codeGeneration.generateComments": true,
  "java.codeGeneration.hashCodeEquals.useInstanceof": true,
  "java.codeGeneration.toString.skipNullValues": true,
  "java.codeGeneration.useBlocks": true,
  // "workbench.colorTheme": "DVLPR",
  "extensions.ignoreRecommendations": true,
  "editor.codeActionsOnSave": {
    // For ESLint
    "source.fixAll.eslint": true,
    // For TSLint
    "source.fixAll.tslint": true,
    // For Stylelint
    "source.fixAll.stylelint": true
  },
  "zenMode.fullScreen": false,
  "workbench.colorCustomizations": {
    "[One Dark Pro (Italic)]": {
      "editor.background": "#282c34"
    },
    "[Palenight Theme]": {
      "editorOverviewRuler.addedForeground": "#0000",
      "editorOverviewRuler.modifiedForeground": "#0000",
      "editorOverviewRuler.deletedForeground": "#0000",
      "editorOverviewRuler.border": "#0000",
      "editorOverviewRuler.wordHighlightForeground": "#0000",
      "editorOverviewRuler.errorForeground": "#0000",
      "editorOverviewRuler.warningForeground": "#0000",
      "editorOverviewRuler.infoForeground": "#0000",
      "editorOverviewRuler.selectionHighlightForeground": "#f000",
      "editor.selectionHighlightBorder": "#f000",
      "editorGroupHeader.tabsBackground": "#31364A",
      "tab.border": "#31364A",
      "editorGroupHeader.noTabsBackground": "#292D3E",
      "activityBar.background": "#292D3E",
      "activityBar.border": "#292D3E",
      "titleBar.activeBackground": "#292D3E",
      "titleBar.inactiveBackground": "#292D3E"
    },
    "[Field Lights]": {
      "titleBar.activeBackground": "#212733",
      "titleBar.inactiveBackground": "#212733",
      "titleBar.border": "#212733"
    },
    "[Forest Night]": {
      "editorGroupHeader.noTabsBackground": "#3c4c55"
    }
  },
  "workbench.activityBar.visible": true,
  "workbench.sideBar.location": "right",
  "window.title": "",
  "editor.cursorSmoothCaretAnimation": true,
  "zenMode.centerLayout": false,
  "zenMode.restore": true,
  "breadcrumbs.filePath": "off",
  "explorer.confirmDragAndDrop": false,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment.line.double-slash",
          "comment",
          "string.quoted",
          "storage.type.function.arrow.js",
          "variable.language.default.js",
          "variable.other.module.js",
          "keyword.operator.relational.js"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
      //    {
      //        "scope": "variable.other.module.js",
      //        "settings": {
      //            "foreground": "#bfc7d5ff",
      //        }
      //    }
    ]
  },
  "breadcrumbs.enabled": false,
  "workbench.colorTheme": "Palenight Theme"
}
